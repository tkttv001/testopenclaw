#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/opencode-cloud@25.1.3/node_modules/opencode-cloud/dist/node_modules:/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/opencode-cloud@25.1.3/node_modules/opencode-cloud/node_modules:/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/opencode-cloud@25.1.3/node_modules:/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/opencode-cloud@25.1.3/node_modules/opencode-cloud/dist/node_modules:/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/opencode-cloud@25.1.3/node_modules/opencode-cloud/node_modules:/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/opencode-cloud@25.1.3/node_modules:/home/tkttv001/.openclaw/workspace/.tools/opencode/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/opencode-cloud@25.1.3/node_modules/opencode-cloud/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/opencode-cloud@25.1.3/node_modules/opencode-cloud/dist/index.js" "$@"
fi
